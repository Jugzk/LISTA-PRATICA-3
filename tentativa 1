# #exercicio 8
def divisors(n: int) -> [int]:

  answer = []

  for num in range(1, n + 1):

      if n % num == 0:

          answer.append(num)

  return answer

def main():

  num = int(input())

  print(divisors(num))

if __name__ == '__main__':

  main()

