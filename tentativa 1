# # exercício 1
print("aqui estão todos os numeros de 1 à 50")
x = 1
while x <= 50:
   print(x)
   x = x + 1

# # exercíco 2
print("FIM")

print("Estes são os números pares de 1 a 100:")

for num in range(1,101):

  if num % 2 == 0:

      print(num)

print("FIM")

# # exercício 3

numero = int(input('Informe o numero para ver a tabuada: '))

print ('\n Tabuada de', numero, ':')

for i in range(1, 11):

  print (numero, 'X', i, '=', (numero * i))

print("FIM")

